    <div class="container" id="pr">
        <h1 class="text-center mt-3 mb-3">
            <%= title %>
        </h1>

        <% if(action=='add' ) { %>

            <% } else { %>

                <div class="card">
                    <div class="card-header">
                        <div class="row">
                            <div class="col">Projects Data</div>
                            <div class="col"></div>
                        </div>
                    </div>
                    <div class="card-body">
                        <div class="table-responsive">
                            <table id="op" class="table table-striped table-bordered">
                                <tr>
                                    <th>Index</th>
                                    <th>Author Name</th>
                                    <th>
                                        <center>Title</center>
                                    </th>
                                    <th>
                                        <center>Type of Project</center>
                                    </th>
                                </tr>
                                <% if(sampleData.length> 0)
                                    {
                                    sampleData.forEach(function(data){
                                    %>
                                    <tr>
                                        <td>
                                            <%= data.id_proj %>
                                        </td>
                                        <td>
                                            <%= data.author_proj %>
                                        </td>
                                        <td>
                                            <%= data.title_proj %>
                                        </td>
                                        <td>
                                            <%= data.type_proj %>
                                        </td>
                                    </tr>
                                    <% }); } else { %>
                                        <tr>
                                            <td colspan="5">No Data Found</td>
                                        </tr>
                                        <% } %>
                            </table>
                        </div>
                    </div>
                </div>

                <% } %>

